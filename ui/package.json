{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "ts-watch": "tsc --noEmit --incremental --watch",
    "clear-cache": "npx rimraf public/cache && npx rimraf app/cache.json && echo \"UI Cache cleared successfully!\"",
    "deploy": "next build --no-lint && touch out/.nojekyll && node ./ghp-postbuild && git add -f out && git commit -m \"Deploy gh-pages\" && cd .. && git subtree push --prefix ui/out origin gh-pages"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.5.0",
    "@simplewebauthn/browser": "^13.2.2",
    "coi-serviceworker": "^0.1.7",
    "comlink": "^4.4.2",
    "next": "14.2.12",
    "o1js": "^2.*",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "xml-js": "^1.6.11",
    "xmldsigjs": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.12",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "type": "module"
}
