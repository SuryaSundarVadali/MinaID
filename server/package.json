{
  "name": "minaid-server",
  "version": "1.0.0",
  "type": "module",
  "description": "MinaID server with WebSocket and Oracle services",
  "main": "websocket-server.js",
  "scripts": {
    "start": "node websocket-server.js",
    "dev": "nodemon websocket-server.js",
    "generate-keys": "tsx generate-keys.ts",
    "oracle": "tsx oracle-server.ts",
    "oracle:dev": "nodemon --exec tsx oracle-server.ts",
    "oracle:prod": "node dist/oracle-server.js",
    "build": "tsc"
  },
  "keywords": [
    "websocket",
    "minaid",
    "real-time",
    "mina-protocol",
    "oracle",
    "passport-verification"
  ],
  "author": "MinaID Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "o1js": "^1.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
